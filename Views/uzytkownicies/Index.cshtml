            </th>
            <th>
                @Html.DisplayNameFor(model => model.ostatnie_logowanie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ip_ostatniego_logowania)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.liczba_znajomych)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.login)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.haslo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nazwisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ksywka)
            </td>
