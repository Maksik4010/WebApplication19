                @Html.DisplayFor(modelItem => item.data_zalozenia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ostatnie_logowanie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ip_ostatniego_logowania)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.liczba_znajomych)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
